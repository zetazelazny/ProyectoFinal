<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a42102578.integrapp.MenuPrincipal"
    android:background="@drawable/habitacion">

    <ImageView
        android:id="@+id/ganaste"
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:src="@drawable/intentaotravez" />
    <ImageView
        android:id="@+id/ojosabiertos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_below="@+id/ganaste"
        android:src="@drawable/perdioojosabiertos"
        android:visibility="visible"
        android:layout_marginTop="14dp"/>
    <ImageView
        android:id="@+id/ojoscerrados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ganaste"
        android:src="@drawable/perdioojoscerrados"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="14dp"/>

</RelativeLayout>
